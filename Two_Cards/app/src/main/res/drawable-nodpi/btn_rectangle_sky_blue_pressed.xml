<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item  >
        <shape
            >
            <stroke android:width="1dp" android:color="#72000000" />
            <padding android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp"
                />
            <corners android:radius="6dp"/>
            <size android:width="90dp" android:height="90dp"/>
        </shape>
    </item>
    <item >
        <shape
            >
            <stroke android:width="1dp" android:color="#4A8CFF"/>
            <padding android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp"
                />
            <corners android:radius="6dp"/>
            <size android:width="90dp" android:height="90dp"/>
        </shape>
    </item>



    <item >
        <shape >

            <gradient
                android:angle="135"
                android:endColor="#95BCFF"
                android:startColor="#4A8BFF"
                />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp"
                />

            <corners android:radius="6dp"/>
            <size android:width="90dp" android:height="90dp"/>
        </shape>
    </item>

    <item >
        <shape>

            <solid android:color="#18FFFFFF"
                />
            <corners android:radius="8dp"/>
        </shape>
    </item>


</layer-list>